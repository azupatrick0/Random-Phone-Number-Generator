{
  "name": "random-phone-number-generator",
  "version": "1.0.0",
  "description": "Random-Phone-Number-Generator is an app that generates phone numbers",
  "scripts": {
    "start": "npm run start:webpack:prod && npm run build && node ./Server/dist/index.js",
    "start:dev:server": "npm run build && nodemon ./Server/dist/index.js",
    "start:dev:client": "npm run start:webpack:dev",
    "start:webpack:dev": "webpack --mode development --watch",
    "start:webpack:prod": "webpack --mode production",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "build": "npm run clean && node ./node_modules/babel-cli/bin/babel.js ./Server/src -d ./Server/dist",
    "lint": "./node_modules/.bin/eslint ./Client --fix --ignore-path .gitignore",
    "clean": "rimraf ./Server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": "10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azupatrick0/Random-Phone-Number-Generator.git"
  },
  "keywords": [
    "Random",
    "Phone",
    "Number",
    "Generator"
  ],
